<?xml version="1.0" encoding="UTF-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- Non focused states -->
    <item android:drawable="@drawable/ic_launcher" android:state_pressed="true"/>
    <item android:drawable="@drawable/ic_launcher" android:state_checked="true"/>
    <item android:drawable="@drawable/ic_launcher" android:state_activated="true"/>
    <item android:drawable="@drawable/ic_launcher" android:state_active="true"/>
    <item android:drawable="@drawable/ic_launcher" android:state_selected="true"/>
    <item android:drawable="@android:color/transparent"></item>

</selector>